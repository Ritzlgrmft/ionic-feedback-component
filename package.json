		"ionic-angular": "^3.0.0",
		"rxjs": "^5.1.1",
		"zone.js": "^0.8.4",
		"@ionic-native/app-version": "^3.0.0",
		"@ionic-native/core": "^3.0.0",
		"@ionic-native/device": "^3.0.0",
		"@ionic-native/screenshot": "^3.0.0",
		"@ionic-native/shake": "^3.0.0",
		"@ionic-native/splash-screen": "^3.0.0",
		"com.darktalker.cordova.screenshot": "https://github.com/gitawego/cordova-screenshot.git",
		"cordova-plugin-app-version": "^0.1.9",
		"cordova-plugin-console": "^1.0.7",
		"cordova-plugin-device": "^1.1.6",
		"cordova-plugin-shake": "git+https://github.com/Ritzlgrmft/cordova-plugin-shake.git",
